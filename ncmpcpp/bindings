def_key "k"         
    scroll_up

def_key "j"
    scroll_down

def_key "g"
    page_up

def_key "G"
    page_down

def_key "home"
    move_home

def_key "end"
    move_end

def_key "d"
    delete_playlist_items

def_key "delete"
    delete_stored_playlist

def_key "l"
    next_column

def_key "h"
    previous_column

def_key "tab"
    next_screen

def_key "L"
    show_lyrics

def_key "space"
    pause

def_key "t"
    jump_to_playing_song

def_key "s"
    toggle_visualization_type
